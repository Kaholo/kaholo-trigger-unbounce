{
  "name": "Unbounce-Trigger",
  "type": "trigger",
  "execProgram": "node",
  "main": "app.js",
  "imgUrl": "logo.png",
  "version": "1.1.0",
  "description": "Handles Unbounce webhooks",
  "methods": [
    {
      "name": "webhookSubmit",
      "route": "post /webhook/unbounce/submit",
      "viewName": "Form Submit",
      "params": [
        {
          "name": "pageName",
          "viewName": "Page Name Pattern",
          "type": "string"
        },
        {
          "name": "pageUrl",
          "viewName": "Page URL Pattern",
          "type": "string"
        },
        {
          "name": "variant",
          "viewName": "Variant",
          "type": "string"
        }
      ]
    }
  ]
}